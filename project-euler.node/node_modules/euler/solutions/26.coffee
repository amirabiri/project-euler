exports.solve = ->
  maxD   = 0
  maxLen = 0

  for d in [1..1000]
    length = unitFractionCycleLength(d)
    if length > maxLen
      maxLen = length
      maxD   = d;

  return maxD

unitFractionCycleLength = (d)->
  result = 1
  prev   = []

  while result != 0
    result = (result * 10) % d
    idx = prev.indexOf(result)

    if idx != -1
      return prev.length - idx

    prev.push result

  return 0
