exports.solve = ->
  leftSide = 999
  num = makePalindrome(leftSide)

  while not check num
    leftSide--
    num = makePalindrome(leftSide)

  return num

makePalindrome = (num)->
  c = num % 10
  b = Math.floor(num / 10) % 10
  a = Math.floor(num / 100)

  return num * 1000 + c * 100 + b * 10 + a

check = (num)->
  return false if num % 11 != 0

  for fac in [999..900]
    return true if num % fac == 0 && num / fac < 1000

  return false
