data      = require 'euler/common/data'
BigNumber = require 'bignumber.js'

numbers = null

exports.initialize = ->
  numbers = data.readFileLines(data.getFilename()).map (line)-> new BigNumber(line)

exports.solve = ->
  sum = new BigNumber(0)
  for number in numbers
    sum = sum.plus number
  return sum.toFixed(0).substr(0, 10)
